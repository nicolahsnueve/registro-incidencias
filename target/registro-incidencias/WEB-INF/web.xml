<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controlador.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>RegistrarIncidenciaServlet</servlet-name>
    <servlet-class>controlador.RegistrarIncidenciaServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>RegistrarIncidenciaServlet</servlet-name>
    <url-pattern>/RegistrarIncidenciaServlet</url-pattern>
</servlet-mapping>
    
    <servlet>
    <servlet-name>ActualizarEstadoServlet</servlet-name>
    <servlet-class>controlador.ActualizarEstadoServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ActualizarEstadoServlet</servlet-name>
    <url-pattern>/ActualizarEstadoServlet</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>EvaluarIncidenciaServlet</servlet-name>
    <servlet-class>controlador.EvaluarIncidenciaServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>EvaluarIncidenciaServlet</servlet-name>
    <url-pattern>/EvaluarIncidenciaServlet</url-pattern>
</servlet-mapping>

</web-app>



